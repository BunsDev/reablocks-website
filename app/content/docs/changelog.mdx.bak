import { useSSG } from 'nextra';
import Mdx from '../../components/ui/mdx';
 
export const getStaticProps = ({ params }) => {
  return fetch(`https://raw.githubusercontent.com/reaviz/reablocks/master/CHANGELOG.md`)
    .then(res => res.text())
    .then(text => ({ props: { ssg: { text } } }))
}
 
export const Result = () => {
  const { text } = useSSG();
  return (
    <Mdx>
      {text}
    </Mdx>
  );
}

<Result />