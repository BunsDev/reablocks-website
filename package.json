    "posthog-js": "^1.131.4",
    "reablocks": "^9.0.0-beta.1",
    "reablocks-docs-theme": "^1.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-syntax-highlighter": "^15.6.1",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
