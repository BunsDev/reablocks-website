    "@storybook/nextjs": "7.6.3",
    "@storybook/react": "7.6.3",
    "@storybook/test": "7.6.3",
    "@storybook/theming": "7.6.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cpx": "^1.5.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.1",
    "nextra-theme-docs": "^4.2.13",
    "postcss": "^8",
